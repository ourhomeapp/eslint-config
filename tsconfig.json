{
  "extends": "@tsconfig/node18-strictest/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": ["esnext"],
    "noEmit": true,
    "target": "esnext"
  },
  "exclude": ["**/node_modules"],
  "ts-node": {
    "transpileOnly": true,
    "compilerOptions": {
      "isolatedModules": false,
      "lib": ["esnext"],
      "module": "commonjs",
      "target": "es2022"
    }
  }
}
