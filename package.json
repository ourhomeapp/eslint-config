{
  "name": "@ourhomeapp/eslint-config",
  "version": "30.2.0",
  "description": "ESLint config for OurHome Web + React projects",
  "author": "OurHome team",
  "license": "MIT",
  "private": false,
  "repository": {
    "url": "git@github.com:ourhomeapp/eslint-config.git",
    "type": "git"
  },
  "main": "index.js",
  "files": [
    "config/**/*",
    "rules/**/*"
  ],
  "scripts": {
    "test": "ava"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register/transpile-only"
    ],
    "files": [
      "test/*.test.*"
    ],
    "concurrency": 5,
    "failFast": true
  },
  "dependencies": {
    "@babel/eslint-parser": "7.16.3",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "eslint-config-airbnb": "19.0.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@types/eslint": "8.2.0",
    "@types/node": "16.11.10",
    "ava": "3.15.0",
    "common-tags": "1.8.2",
    "eslint": "8.3.0",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  },
  "peerDependencies": {
    "@babel/core": ">=7.16.x",
    "@babel/preset-env": ">=7.16.x",
    "@babel/preset-react": ">=7.16.x",
    "eslint": ">=8.3.x",
    "prettier": ">=2.4.x",
    "typescript": ">=4.5.x"
  }
}
